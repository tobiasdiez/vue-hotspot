{
  "name": "vue-hotspot",
  "version": "2.0.3",
  "description": "Hotspot component for Vue.js.",
  "author": "James Wang",
  "scripts": {
    "serve": "vue-demi-switch 2 && vue-cli-service serve",
    "serve:vue3": "vue-demi-switch 3 vue3 && vue-cli-service serve",
    "build": "npm run build:vue2 && npm run build:vue3",
    "build:vue2": "vue-demi-switch 2 && cross-env VUE_VERSION=vue2 NODE_ENV=production rollup -c",
    "build:vue3": "vue-demi-switch 3 vue3 && cross-env VUE_VERSION=vue3 NODE_ENV=production rollup -c",
    "lint": "vue-cli-service lint",
    "demo": "vue-demi-switch 2 && vue-cli-service build",
    "demo:vue3": "vue-demi-switch 3 vue3 && vue-cli-service build",
    "pub": "sh ./publish.sh",
    "test:unit": "vue-cli-service test:unit --coverage && codecov",
    "test": "vue-demi-switch 3 vue3 && vue-cli-service test:unit"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "main": "dist/vue2/vue-hotspot.js",
  "module": "components/VueHotspot.vue",
  "dependencies": {
    "core-js": "^2.6.5",
    "rollup-plugin-postcss": "^4.0.2",
    "vue-demi": "^0.13.11"
  },
  "peerDependencies": {
    "@vue/composition-api": "^0.3.2",
    "vue": "^2.6.10 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^4.0.5",
    "@vue/cli-service": "^4.5.19",
    "@vue/compiler-sfc": "^3.2.39",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.0.0",
    "codecov": "^3.5.0",
    "cross-env": "^5.2.1",
    "eslint": "^5.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^4.3.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "rollup": "^1.20.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue2": "npm:rollup-plugin-vue@5.1.9",
    "rollup-plugin-vue3": "npm:rollup-plugin-vue@6.0.0",
    "standard": "^14.1.0",
    "vue-template-compiler": "^2.6.10",
    "vue": "^2.6.10",
    "vue3": "npm:vue@3",
    "@vue/composition-api": "^1.7.1"
  },
  "bugs": {
    "url": "https://github.com/jamesxwang/vue-hotspot/issues"
  },
  "homepage": "https://github.com/jamesxwang/vue-hotspot#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "keywords": [
    "vue",
    "hotspot",
    "component"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesxwang/vue-hotspot.git"
  }
}
